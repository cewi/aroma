{
    "name": "gourmet/aroma"
    , "description": "DB-based configuration for CakePHP 4."
    , "type": "cakephp-plugin"
    , "keywords": [
        "cakephp"
        , "gourmet"
    ]
    , "homepage": "https://github.com/gourmet/aroma"
    , "license": "MIT"
    , "authors": [
        {
            "name": "Jad Bitar"
            , "homepage": "http://jadb.io"
            , "role": "Author"
        }
        , {
            "name": "Others"
            , "homepage": "https://github.com/gourmet/aroma/graphs/contributors"
        }
    ]
    , "support": {
        "issues": "https://github.com/gourmet/aroma/issues"
        , "source": "https://github.com/gourmet/aroma"
    }
    , "require": {
        "cakephp/cakephp": "~4.2.0"
    }
    , "require-dev": {
        "phpunit/phpunit": "~8.5.0 || ^9.3",
        "cakephp/cakephp-codesniffer": "~4.2.0"
    }
    , "autoload": {
        "psr-4": {
            "Gourmet\\Aroma\\": "src"
        }
    }
    , "autoload-dev": {
        "psr-4": {
            "Gourmet\\Aroma\\Test\\": "tests"
        }
    }
    , "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }

}
